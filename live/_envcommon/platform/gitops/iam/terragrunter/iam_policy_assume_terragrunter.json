{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowSpecifics",
      "Action": [
        "application-autoscaling:*",
        "autoscaling:*",
        "cloudfront:*",
        "cloudtrail:*",
        "cloudwatch:*",
        "dynamodb:*",
        "ec2:*",
        "ecr:*",
        "ecs:*",
        "eks:*",
        "elasticache:*",
        "elasticloadbalancing:*",
        "es:*",
        "events:*",
        "firehose:*",
        "grafana:*",
        "iam:*",
        "kms:*",
        "logs:*",
        "rds:*",
        "route53:*",
        "s3:*",
        "secretsmanager:*",
        "sns:*",
        "sqs:*"
      ],
      "Effect": "Allow",
      "Resource": [
        "*"
      ]
    },
    {
      "Sid": "DenySpecifics",
      "Action": [
        "ec2:*ReservedInstances*",
        "iam:*Group*",
        "iam:*Login*",
        "iam:*User*"
      ],
      "Effect": "Deny",
      "Resource": [
        "*"
      ]
    }
  ]
}